<h3>Add Transaction</h3>
<form id="add-transaction-form" action="/transactions" method="POST">
{{>_add_transaction}}
    <div class="row">
        <div class="col s6">
            <a href="/transactions/add" class="btn-floating waves-effect waves-light red darken-2"><i class="material-icons">add</i></a>
            </div>
        <div class="col s6 right-align save-cancel">
            <input type="submit" value="Save" class="btn">
            <a href="/dashboard" class="btn orange darken-2">Cancel</a>
        </div>
    </div>
</form>
